{% extends "RizzaCalendarBundle::layout.html.twig" %}
{% block content %}
<div class="event_list">
    <ul>
        {% for event in events %}
        <li><a href="{{ path(rizza_calendar_route('event', 'show'), {'id': event.getId()} ) }}">{{ event.getTitle() }}</li>
        {% endfor %}
    </ul>
</div>
{% endblock content %}