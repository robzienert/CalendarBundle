{% extends "RizzaCalendarBundle::layout.html.twig" %}
{% block content %}
<form action="{{ path(rizza_calendar_route('calendar', 'add')) }}" {{ form_enctype(form) }} method="POST">
    {{ form_errors(form) }}

    {{ form_row(form.name) }}

    {{ form_rest(form) }}
    
    <input type="submit" value="Create Calendar" />
</form>
{% endblock content %}